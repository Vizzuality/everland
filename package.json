{
  "name": "everland",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next export",
    "check-types": "tsc --noemit -p tsconfig.json",
    "test": "start-server-and-test 'yarn dev' 3000 'yarn cypress:run'",
    "storybook:styles:dev": "NODE_ENV=development tailwind build ./src/styles/globals.css -o ./public/styles/tailwind.storybook.css",
    "storybook:styles:prod": "NODE_ENV=production tailwind build ./src/styles/globals.css -o ./public/styles/tailwind.storybook.css",
    "storybook:dev": "yarn storybook:styles:dev && start-storybook -p 6006 -s ./public -c .storybook",
    "storybook:build": "yarn storybook:styles:prod && build-storybook -s ./public -c .storybook",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --headless --browser chrome",
    "prepare": "husky install"
  },
  "dependencies": {
